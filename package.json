	"scripts": {
		"o!!!!!!!!": "",
		"o!!!!!!!!1": "Main",
		"o!!!!!!!!2": "",
		"dev": "svelte-kit dev",
		"dev--david": "concurrently npm:firebase vite",
		"build": "svelte-kit build",
		"oo---------": "",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync && npm run smui-theme-light && npm run smui-theme-dark",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"o!!!!!!": "",
		"o!!!!!!1": "Test",
		"o!!!!!!2": "",
		"test": "npm run test-jest",
		"test-jest": "jest --detectOpenHandles",
		"test-jest-watch": "jest --watchAll",
		"test-vitest": "vitest ",
		"test-coverage": "vitest run --coverage",
		"test-mocha": "mocha --exit test/firestore.spec.js",
		"test-mocha-2": "mocha --exit src/lib/firebase/tdd/firestore.spec.js",
		"test-db": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"o!!!!!!!!!!": "",
		"o!!!!!!!!!1": "UI - Material Design + SASS",
		"o!!!!!!!!!2": "",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"o!!!!!": "",
		"o!!!!!1": "Firebase - EMULTAORS",
		"o!!!!!2": "",
		"emu-exec-vitest": "firebase emulators:exec --only firestore 'npm run test-vitest' ",
		"emu": "firebase emulators:start --only firestore --project firestore-ax-test",
		"emu-kill": "kill-port --port 9099,8080 && firebase emulators:start --only firestore --project firestore-ax-test",
		"emu--david-east": "firebase emulators:start --only auth,firestore,hosting --import=./offline-seed --export-on-exit --project calmban-dev",
		"emu--import-prod": "firebase --project=webagency-2021 emulators:start --only firestore --import ./src/db/firebase/back/script/sync-prod-emu/bk/bk",
		"o!!!!!3": "",
		"o!!!!!4": "Firebase - DEPLOY",
		"o!!!!!5": "",
		"deploy": "npm run deploy-hosting",
		"deploy-hosting": "firebase deploy --only hosting --project default",
		"deploy-firestore": "firebase deploy --only firestore ",
		"deploy-full": "npm run deploy-v3-full",
		"deploy-v3": "firebase hosting:channel:deploy v3",
		"deploy-v3-full": "firebase deploy --only hosting,firestore ",
		"deploy-v2-full": "firebase deploy --only hosting,firestore",
		"deploy-v2": "firebase deploy --only hosting,firestore",
		"ooooo---------": "",
		"db-script": "npm run script-csvToFirebase",
		"db-script-deb": "npm run script-concat-deb",
		"db-script-concat-deb": "cd SCRIPTS && ndb index-concatAllDb.js",
		"db-script-concat": "cd SCRIPTS && node ./index-concatAllDb.js",
		"db-script-csvToJson": "cd SCRIPTS && node ./1-index--convert-csv-to-json.js --filename='all-listado-MS - bec' && node ./1-index--convert-csv-to-json.js --filename='all-listado-MS - cursos' && node ./1-index--convert-csv-to-json.js --filename='all-listado-MS - danz' && node ./1-index--convert-csv-to-json.js --filename='all-listado-MS - exp' && node ./1-index--convert-csv-to-json.js --filename='all-listado-MS - fest' && node ./1-index--convert-csv-to-json.js --filename='all-listado-MS - lib' && node ./1-index--convert-csv-to-json.js --filename='all-listado-MS - P' && node ./1-index--convert-csv-to-json.js --filename='all-listado-MS - rev' && node ./1-index--convert-csv-to-json.js --filename='all-listado-MS - z'",
		"db-script-importLocalJsonToFirebase": "cd SCRIPTS && node --experimental-json-modules ./2-index--import-localJson-To-Firebase.js --tablename=XXX",
		"oooooo---------": "",
		"sync-prod-to-local": "./src/db/firebase/back/script/sync-prod-emu/start-up.sh",
		"firestore-start": "node ./scripts/injectFirestoreBoguide.js",
		"firestore-debug": "nodemon --inspect ./scripts/injectFirestoreBoguide.js"
	},
