{
	"hosting": {
		"public": "build",
		"cleanUrls": true,
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
	},
	"firestore": {
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	},
	"functions": {
		"source": "0-functions",
		"predeploy": "npm --prefix \"$RESOURCE_DIR\" run build"
	},
	"emulators": {
		"auth": {
			"port": 9099
		},
		"functions": {
			"port": 5001
		},
		"firestore": {
			"port": 8080
		},
		"hosting": {
			"port": 5000
		},
		"ui": {
			"enabled": false
		}
	}
}
